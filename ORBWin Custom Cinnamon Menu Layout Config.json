    "pref_overlay_key": {
        "type": "keybinding",
        "description": "Keyboard shortcut to open and close the menu",
        "default": "Super_L::Super_R",
        "value": "Super_L::Super_R"
    },
    "pref_use_a_custom_icon_for_applet": {
        "type": "switch",
        "default": false,
        "description": "Use a custom icon",
        "tooltip": "Unchecking this allows the theme to set the icon",
        "value": false
    },
    "pref_custom_icon_for_applet": {
        "type": "iconfilechooser",
        "expand-width": true,
        "default": "custom-start-here-linux-mint-symbolic",
        "description": "Icon",
        "tooltip": "Select an icon to show in the panel.",
        "dependency": "pref_use_a_custom_icon_for_applet",
        "value": "custom-start-here-linux-mint-symbolic"
    },
    "pref_custom_label_for_applet": {
        "type": "entry",
        "expand-width": true,
        "default": "Menu",
        "description": "Custom label",
        "tooltip": "Enter custom text to show in the panel.",
        "value": ""
    },
    "pref_open_menu_on_hover": {
        "type": "switch",
        "default": false,
        "description": "Open the menu on mouse over",
        "tooltip": "Enable opening the menu when the mouse enters the applet.",
        "value": false
    },
    "pref_menu_hover_delay": {
        "type": "spinbutton",
        "default": 0,
        "min": 0,
        "max": 1000,
        "step": 50,
        "dependency": "pref_open_menu_on_hover",
        "units": "milliseconds",
        "description": "Menu hover delay:",
        "tooltip": "Delay before the menu is opened on entering the applet.",
        "value": 0
    },
    "pref_display_category_icons": {
        "type": "switch",
        "default": true,
        "description": "Show category icons",
        "tooltip": "Choose whether or not to show icons on categories.",
        "value": false
    },
    "pref_display_application_icons": {
        "type": "switch",
        "default": true,
        "description": "Show application icons",
        "tooltip": "Choose whether or not to show icons on applications.",
        "value": true
    },
    "pref_display_fav_box": {
        "type": "switch",
        "default": true,
        "description": "Show favorites and quit options",
        "tooltip": "Choose whether or not to show the left pane of the menu.",
        "value": false
    },
    "pref_show_places": {
        "type": "switch",
        "default": true,
        "description": "Show bookmarks and places",
        "tooltip": "Choose whether or not to show bookmarks and places in the menu.",
        "value": true
    },
    "pref_show_recents_button": {
        "type": "switch",
        "default": true,
        "description": "Show \"Recent documents\" category",
        "tooltip": "For people who want the \"Recent documents\" category hidden without disabling recent documents globally.",
        "value": true
    },
    "pref_max_recent_files": {
        "type": "spinbutton",
        "default": 20,
        "min": 5,
        "max": 100,
        "step": 1,
        "units": "documents",
        "description": "Max recent documents:",
        "tooltip": "",
        "value": 20
    },
    "pref_enable_autoscroll": {
        "type": "switch",
        "default": true,
        "description": "Enable auto-scrolling in application list",
        "tooltip": "Choose whether or not to enable smooth auto-scrolling in the application list.",
        "value": true
    },
    "pref_animate_menu": {
        "type": "switch",
        "default": false,
        "description": "Animate open/close menu",
        "tooltip": "",
        "value": false
    },
    "pref_apps_info_box_alignment_to_the_left": {
        "type": "switch",
        "default": false,
        "description": "Align application info box text to the left",
        "tooltip": "",
        "value": false
    },
    "pref_fuzzy_search_enabled": {
        "type": "switch",
        "default": false,
        "description": "Enable fuzzy search (EXPERIMENTAL)",
        "tooltip": "This option enables an infinitelly better sorting of search results. It's marked as experimental because it has a huge impact on performance every time the search box is cleared.",
        "value": true
    },
    "pref_disable_new_apps_highlighting": {
        "type": "switch",
        "default": true,
        "description": "Disable newly installed applications highlighting",
        "tooltip": "",
        "value": true
    },
    "pref_hide_allapps_category": {
        "type": "switch",
        "default": false,
        "description": "Hide \"All Applications\" category",
        "tooltip": "",
        "value": true
    },
    "pref_display_favorites_as_category_menu": {
        "type": "switch",
        "dependency": "pref_hide_allapps_category",
        "default": false,
        "description": "Display \"Favorites\" as a category menu",
        "tooltip": "",
        "value": true
    },
    "pref_hide_applications_list_scrollbar": {
        "type": "switch",
        "default": false,
        "description": "Hide applications list scrollbar",
        "tooltip": "",
        "value": false
    },
    "pref_swap_categories_box": {
        "type": "switch",
        "default": false,
        "description": "Swap categories box",
        "tooltip": "By default, the categories box is to the left of the applications list. With this option enabled, the categories box will be placed to the right of the applications list.",
        "value": true
    },
    "pref_cat_hover_delay": {
        "type": "spinbutton",
        "default": 0,
        "min": 0,
        "max": 1000,
        "step": 50,
        "units": "milliseconds",
        "description": "Category selection delay:",
        "tooltip": "Delay between switching categories.",
        "value": 0
    },
    "pref_appinfo_display_method": {
        "type": "combobox",
        "default": "infobox",
        "description": "Display method for information about applications:",
        "options": {
            "None": "none",
            "Tooltips": "tooltip",
            "Info box": "infobox"
        },
        "value": "tooltip"
    },
    "pref_privilege_elevator": {
        "type": "combobox",
        "default": "gksu",
        "description": "Privileges elevator:",
        "options": {
            "gksu": "gksu",
            "gksudo": "gksudo"
        },
        "tooltip": "The graphical front-end to use to run applications as root.\nThis is used by the \"Open as Root\" context menu and the option \"Gain privileges of not owned files\".",
        "value": "gksu"
    },
    "pref_terminal_emulator": {
        "type": "entry",
        "expand-width": true,
        "default": "x-terminal-emulator",
        "description": "Terminal emulator:",
        "tooltip": "Choose the terminal emulator used by all launch from terminal options.\nIMPORTANT!!! The terminal emulator has to support the -e argument.",
        "value": "x-terminal-emulator"
    },
    "pref_system_buttons_display": {
