    "pref_show_desktop_file_folder_on_context": {
        "type": "switch",
        "default": false,
        "description": "Show \"Open .desktop file folder\" on context",
        "value": false
    },
    "pref_show_edit_desktop_file_on_context": {
        "type": "switch",
        "default": false,
        "description": "Show \"Edit .desktop file\" on context",
        "tooltip": "It adds to the applications context menu an entry that will allow you to open the current selected application's .desktop file in a text editor.",
        "value": false
    },
    "pref_gain_privileges_on_context": {
        "type": "switch",
        "dependency": "pref_show_edit_desktop_file_on_context",
        "default": false,
        "description": "Gain privileges of not owned files",
        "tooltip": "If enabled, this options will open as root .desktop files that aren't owned by the current user.\n\nThis isn't needed for all text editors. Some text editors will allow you to open and edit not owned files and will ask for the root password at the moment of saving the changes.",
        "value": false
    },
    "pref_custom_editor_for_edit_desktop_file_on_context": {
        "type": "filechooser",
        "dependency": "pref_show_edit_desktop_file_on_context",
        "select-dir": false,
        "default": "",
        "description": "Custom text editor:",
        "tooltip": "Set a custom text editor to open .desktop files (just the name of the text editor executable or the path to it).\nLeave it blank to let the system decide.",
        "value": ""
    },
    "pref_remember_recently_used_apps": {
        "type": "switch",
        "default": false,
        "description": "Show \"Recent Applications\" category",
        "tooltip": "From the moment this option is enabled, every application launched from the menu will be stored and displayed in the \"Recent Applications\" category.\nEvery time this option is disabled, the list of recently used applications will be cleared.",
        "value": false
    },
    "pref_recent_apps_ignore_favorites": {
        "type": "switch",
        "default": false,
        "dependency": "pref_remember_recently_used_apps",
        "description": "Ignore Favorites",
        "tooltip": "If enabled, applications set as Favorite will not be stored in the \"Recent Applications\" category.",
        "value": false
    },
    "pref_recently_used_apps_separator": {
        "type": "switch",
        "default": false,
        "dependency": "pref_remember_recently_used_apps",
        "description": "Display separator after \"Recent Applications\" category (WARNING)",
        "tooltip": "This option, when enabled, will break the menu keyboard navigation. Only enable this option if you will never use the keyboard to navigate the menu.\nThis message is only temporary until I figure out how to fix this.",
        "value": false
    },
    "pref_recently_used_apps_invert_order": {
        "type": "switch",
        "default": false,
        "dependency": "pref_remember_recently_used_apps",
